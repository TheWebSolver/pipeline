{
    "name": "thewebsolver/pipeline",
    "description": "Follows Chain of Responsibility Design Pattern to handle the given subject/request.",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "keywords": ["psr-7", "psr-15", "pipeline", "bridge"],
    "require": {
      "php": ">=8.2.0"
    },
    "require-dev": {
      "phpunit/phpunit": "9.6.10",
      "yoast/phpunit-polyfills": "1.1.0"
    },
    "autoload": {
      "psr-4": {
        "TheWebSolver\\Codegarage\\Lib\\": "Src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "TheWebSolver\\Codegarage\\": "Tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
